{
    "family": "nodejs-webserver",
    "containerDefinitions": [
        {
            "image": "783858137024.dkr.ecr.us-east-1.amazonaws.com/jenkins-docker-ecr:latest",
            "name": "nodejs",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::783858137024:role/ecsTaskExecutionRole" 
}
